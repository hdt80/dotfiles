# Raise volume
"amixer sset Master 5%+"
	XF86AudioRaiseVolume

# Lower volume
"amixer sset Master 5%-"
	XF86AudioLowerVolume

# Toggle mute
"amixer sset Master toggle"
	XF86AudioMute

# Brightness up
"xbacklight -inc 5"
	XF86KbdBrightnessUp

# Brightness down
"xbacklight -dec 5"
	XF86KbdBrightnessDown
